{
  "name": "resume-customizer-api",
  "version": "1.1.0",
  "description": "AI-powered resume customization API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "worker": "node worker.js",
    "dev:worker": "nodemon worker.js",
    "dev:format-worker": "nodemon workers/formattedResumeProcessor.js",
    "test": "mocha --require './tests/setup.js' --recursive 'tests/**/*.test.js'",
    "test:coverage": "nyc npm test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate:dev": "sequelize db:migrate",
    "migrate:prod": "sequelize db:migrate --env production",
    "seed:dev": "sequelize db:seed:all",
    "seed:prod": "sequelize db:seed:all --env production",
    "docker:build": "docker build -t resume-customizer-api .",
    "docker:run": "docker run -p 3000:3000 resume-customizer-api"
  },
  "dependencies": {
    "@mozilla/readability": "^0.4.4",
    "ajv": "^8.12.0",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.10.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-prom-bundle": "^6.6.0",
    "express-rate-limit": "^6.9.0",
    "express-validator": "^7.0.1",
    "fs-extra": "^11.2.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "jsdom": "^22.1.0",
    "jsonwebtoken": "^9.0.1",
    "mammoth": "^1.6.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.14.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "prom-client": "^14.2.0",
    "sequelize": "^6.32.1",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "eslint": "^8.46.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.6.1",
    "sinon": "^15.2.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/resume-customizer.git"
  },
  "author": "Your Name <your.email@example.com>",
  "license": "MIT"
}
