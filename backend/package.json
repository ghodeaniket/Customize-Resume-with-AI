{
  "name": "backend",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "worker": "node worker.js",
    "test": "mocha --require './tests/setup.js' --recursive 'tests/**/*.test.js'",
    "test:unit": "mocha --require './tests/setup.js' --recursive 'tests/unit/**/*.test.js'",
    "test:integration": "mocha --require './tests/setup.js' --recursive 'tests/integration/**/*.test.js'",
    "test:e2e": "mocha --require './tests/setup.js' 'tests/e2e/**/*.test.js'",
    "test:coverage": "nyc npm test",
    "test:unit:coverage": "nyc npm run test:unit",
    "test:integration:coverage": "nyc npm run test:integration",
    "test:ci": "nyc --reporter=lcov npm test && nyc report --reporter=text-lcov > coverage/lcov.info",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Resume Customizer API",
  "dependencies": {
    "@mozilla/readability": "^0.6.0",
    "axios": "^1.8.4",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "docx": "^9.3.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.0",
    "joi": "^17.13.3",
    "jsdom": "^26.0.0",
    "mammoth": "^1.9.0",
    "morgan": "^1.10.0",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.16.0",
    "pg": "^8.14.1",
    "sequelize": "^6.37.6",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "jest": "^29.7.0",
    "mocha": "^10.4.0",
    "nodemon": "^3.1.9",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.6.2",
    "sinon": "^17.0.1",
    "supertest": "^7.1.0"
  }
}
